#Region "Public"

    ''' <summary>
    ''' Returns a random word of specified type.
    ''' </summary>
    ''' <param name="typeofword">The type of word to get.</param>
    ''' <returns>A random word, with its type specified.</returns>
    ''' <remarks></remarks>
    Public Shared Function GetRandom(ByVal typeofword As WordType) As String
        Select Case typeofword
            Case WordType.Verb
                Return GetRandomVerb()
            Case WordType.Adjective
                Return GetRandomAdjective()
            Case WordType.Noun
                Return GetRandomNoun()
            Case WordType.Adverb
                Return GetRandomAdverb()
            Case WordType.Pronoun
                Return GetRandomPronoun()
            Case WordType.Preposition
                Return GetRandomPreposition()
            Case WordType.Conjunction
                Return GetRandomConjunction()
            Case WordType.Interjection
                Return GetRandomInterjection()
            Case WordType.Compound
                Return GetRandomCompound()
        End Select
    End Function

    ''' <summary>
    ''' Replaces the set keywords with their random equivalents.
    ''' </summary>
    ''' <param name="txt">The text to be updated.</param>
    ''' <returns>String with all keywords replaced with random equivalents.</returns>
    ''' <remarks></remarks>
    Public Shared Function UpdateString(ByVal txt As String) As String
        Dim tem As String = txt
        While tem.IndexOf("{verb}") <> -1
            tem = Replace(tem, "{verb}", GetRandomVerb(), , 1)
        End While

        While tem.IndexOf("{adjective}") <> -1
            tem = Replace(tem, "{adjective}", GetRandomAdjective(), , 1)
        End While

        While tem.IndexOf("{noun}") <> -1
            tem = Replace(tem, "{noun}", GetRandomNoun(), , 1)
        End While

        While tem.IndexOf("{adverb}") <> -1
            tem = Replace(tem, "{adverb}", GetRandomAdverb(), , 1)
        End While

        While tem.IndexOf("{pronoun}") <> -1
            tem = Replace(tem, "{pronoun}", GetRandomPronoun(), , 1)
        End While

        While tem.IndexOf("{preposition}") <> -1
            tem = Replace(tem, "{preposition}", GetRandomPreposition(), , 1)
        End While

        While tem.IndexOf("{conjunction}") <> -1
            tem = Replace(tem, "{conjunction}", GetRandomConjunction(), , 1)
        End While

        While tem.IndexOf("{interjection}") <> -1
            tem = Replace(tem, "{interjection}", GetRandomInterjection(), , 1)
        End While

        While tem.IndexOf("{compound}") <> -1
            tem = Replace(tem, "{compound}", GetRandomCompound(), , 1)
        End While

        Return tem
    End Function

#End Region

